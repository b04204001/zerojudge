
#include <iostream>
#include <string>
using namespace std;
int main(void){
	string s;
	cout<<"          " << "NAME" << "                     " << "SOUNDEX CODE" <<endl;
	while(cin >>s){
		string t;
		int forward=0;
		t = s[0];
		int j=1;
		for(int i=1;i<s.length();i++){
			if(s[i] == 'A' || s[i] == 'E' || s[i] == 'I' || s[i] == 'O' || s[i] == 'U' || s[i] == 'Y' || s[i] == 'W' || s[i] == 'H'){
				forward =0;
				continue;
			}
			else{
				if(s[i] == 'B' || s[i] == 'P' || s[i] == 'F' || s[i] == 'V' || forward==1 ){ //B, P, F, V
					forward =1;
					t += '1';
					j++;
				}
				else if(s[i] == 'C' || s[i] == 'S' || s[i] == 'K' || s[i] == 'G' || s[i] == 'J' || s[i] == 'Q' || s[i] == 'X' || s[i] == 'Z' || forward==2 ){ //C, S, K, G, J, Q, X, Z
					forward=2;
					t+= '2';
					j++;
				}
				else if(s[i] == 'D' || s[i] == 'T' || forward==3 ){ //D, T
					forward=3;
					t+= '3';
					j++;
				}
				else if(s[i] == 'L' || forward==4 ){
					forward=4;
					t+='4';
					j++;
				}
				else if(s[i] == 'M' || s[i] == 'N' || forward==5 ){
					forward=5;
					t+='5';
					j++;
				}
				else if(s[i] == 'R' || forward==6 ){
					forward =6;
					t+='6';
					j++;
				}
			}
		}
		while(t.length() <4){
			t +='0';
		}
		cout<< "          " << s ;
		int n= 25-s.length();
		for(int i=0;i<n;i++){
			cout << ' ';
		}
		for(int i=0;i<4;i++){
			cout<< t[i] ;
		}
		cout <<endl;
		
	}
	cout << "                    " << "END OF OUTPUT" ;
    return 0;
}

 	

 
 	

 
